PREFIX plat: <http://matonto.org/ontologies/platform/config#>

SELECT
    ?id
    (GROUP_CONCAT(DISTINCT ?resource ; separator = ",") AS ?resources)
WHERE {
    ?id a plat:State ;
        plat:forUser ?userId ;
        plat:stateResource ?resource .

	FILTER EXISTS { ?id plat:application ?application } .
} GROUP BY ?id