@prefix : <http://matonto.org/ontologies/delimited#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/>.
@prefix mcat: <http://matonto.org/ontologies/catalog#> .

<http://matonto.org/ontologies/delimited> a owl:Ontology ;
    owl:imports <http://matonto.org/ontologies/catalog> ;
    owl:versionIRI <http://matonto.org/ontologies/delimited#1.2>;
    rdfs:label "The MatOnto Delimited Ontology"@en ;
    rdfs:comment "The MatOnto delimited ontology defines MatOnto Mappings and their records in the MatOnto catalog."@en ;
    dct:creator [foaf:name "Bryan Miller"], [foaf:name "Megan Mercer"];
    foaf:maker [foaf:name "MatOnto"; foaf:homepage <http://matonto.org>];
    dct:modified "2017-06-23"^^xsd:date.

# MappingRecord

:MappingRecord a owl:Class;
    rdfs:subClassOf mcat:VersionedRDFRecord;
    rdfs:label "Mapping"@en;
    rdfs:comment "A collection of RDF data that defines a mapping resource."@en.

# Mapping

:Mapping rdf:type owl:Class ;
    rdfs:label "Mapping" ;
    rdfs:comment "An object that represents a mapping configuration." .

:sourceOntology a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Mapping ;
    rdfs:range owl:Ontology ;
    rdfs:label "source ontology" ;
    rdfs:comment "Defines the ontology that the Mapping is based on." .

:sourceRecord a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Mapping ;
    rdfs:range mcat:OntologyRecord ;
    rdfs:label "source record" ;
    rdfs:comment "Defines the OntologyRecord that the Mapping is based on." .

:sourceBranch a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Mapping ;
    rdfs:range mcat:Branch ;
    rdfs:label "source branch" ;
    rdfs:comment "Defines the Branch of the OntologyRecord that the Mapping is based on." .

:sourceCommit a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Mapping ;
    rdfs:range mcat:Commit ;
    rdfs:label "source commit" ;
    rdfs:comment "Defines the Commit of the OntologyRecord that the Mapping is based on." .

:versionIRI a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Mapping ;
    rdfs:range xsd:anyURI ;
    rdfs:label "version IRI" ;
    rdfs:comment "The IRI of the current version of a Mapping." .

:defaultPrefix a owl:DatatypeProperty ;
    rdfs:subPropertyOf owl:topDataProperty ;
    rdfs:domain :Mapping ;
    rdfs:range xsd:string ;
    rdfs:label "default prefix" ;
    rdfs:comment "The default prefix used for the Mapping" .

# ClassMapping

:ClassMapping a owl:Class ;
    rdfs:label "Class Mapping" ;
    rdfs:comment "An object that represents the creation of a specific type of individual." .

:dataProperty a owl:ObjectProperty ;
    rdfs:subPropertyOf :propertyMapping ;
    rdfs:domain :ClassMapping ;
    rdfs:range :DataMapping ;
    rdfs:label "data property" ;
    rdfs:comment "A data property mapping for a ClassMapping." .

:mapsTo a owl:ObjectProperty ;
    rdfs:domain :ClassMapping ;
    rdfs:range owl:Thing ;
    rdfs:label "maps to" ;
    rdfs:comment "Defines the type that will be used when defining an object from this class mapping" .

:objectProperty a owl:ObjectProperty ;
    rdfs:subPropertyOf :propertyMapping ;
    rdfs:domain :ClassMapping ;
    rdfs:range :ObjectMapping ;
    rdfs:label "object property" ;
    rdfs:comment "An object property mapping for a ClassMapping." .

:hasPrefix a owl:DatatypeProperty ;
    rdfs:domain :ClassMapping ;
    rdfs:range xsd:string ;
    rdfs:label "has prefix" ;
    rdfs:comment "The prefix used if the Cell is an instance" .

:localName a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :ClassMapping ;
    rdfs:range xsd:string ;
    rdfs:label "local name" ;
    rdfs:comment "The pattern specifying the local name of the generated instance. A value of \"${UUID}\" results in the generation of random UUID. A digit value results in the value for the cell in that column being inserted into the IRI (e.g. ${2} would be the value for the third column in the row)" .

# PropertyMapping

:PropertyMapping a owl:Class ;
    rdfs:label "Property Mapping" ;
    rdfs:comment "An object used to relate a ClassMapping to a property on the generated instance." .

# ObjectMapping

:ObjectMapping a owl:Class ;
    rdfs:subClassOf :PropertyMapping ;
    rdfs:label "Object Mapping" ;
    rdfs:comment "An object used to relate a ClassMapping to an Object Property on the generated instance." .

:classMapping a owl:ObjectProperty ;
    rdfs:domain :ObjectMapping ;
    rdfs:range :ClassMapping ;
    rdfs:label "class mapping" ;
    rdfs:comment "The ClassMapping that describes the target resource." .

# DataMapping

:DataMapping a owl:Class ;
    rdfs:subClassOf :PropertyMapping ;
    rdfs:label "Data Mapping" ;
    rdfs:comment "An object used to relate a ClassMapping to a Datatype Property on the generated instance." .

# Misc.

:hasProperty a owl:ObjectProperty ;
	rdfs:domain :DataMapping , :ObjectMapping , :PropertyMapping ;
	rdfs:range rdf:Property ;
    rdfs:label "has property" ;
	rdfs:comment "The Property defines the relationship for the PropertyMapping" .

:columnIndex a owl:DatatypeProperty ;
    rdfs:domain :Cell , :DataMapping , :ObjectMapping ;
    rdfs:range xsd:integer ;
    rdfs:label "column index" ;
    rdfs:comment "The cells column. Starting at 0." .

:propertyMapping a owl:ObjectProperty ;
    rdfs:range :PropertyMapping ;
    rdfs:label "property mapping" ;
    rdfs:comment "A property mapping, either data or object, for a ClassMapping." .

rdf:Property a owl:Class .
