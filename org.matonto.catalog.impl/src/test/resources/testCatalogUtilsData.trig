@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix mcat: <http://matonto.org/ontologies/catalog#> .
@prefix matuser: <http://matonto.org/ontologies/user/management#> .
@prefix : <http://test.com#> .

GRAPH <http://test.com#empty> { }

GRAPH <http://test.com#random> {
    :random dc:title "Title" .
}

GRAPH <http://test.com#different> {
    :different a owl:Class .
}

GRAPH <http://test.com#catalog> {
    :catalog a mcat:Catalog .
}

GRAPH <http://test.com#record> {
    :record a mcat:Record ;
        mcat:catalog :catalog .
}

GRAPH <http://test.com#record-no-catalog> {
    :record-no-catalog a mcat:Record .
}

GRAPH <http://test.com#versionedRDFRecord> {
    :record a mcat:VersionedRDFRecord ;
        mcat:catalog :catalog ;
        mcat:branch :branch .
}

GRAPH <http://test.com#versionedRDFRecord-no-catalog> {
    :versionedRDFRecord-no-catalog a mcat:VersionedRDFRecord .
}

GRAPH <http://test.com#versionedRDFRecord-no-branch> {
    :versionedRDFRecord-no-branch a mcat:VersionedRDFRecord ;
        mcat:catalog :catalog .
}

GRAPH <http://test.com#branch> {
    :branch a mcat:Branch .
}