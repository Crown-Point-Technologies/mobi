CONSTRUCT { ?s ?pred ?obj }
FROM %ADDITIONS_GRAPH%
FROM %DELETIONS_GRAPH%
FROM NAMED %DELETIONS_GRAPH%
WHERE {
    {

        SELECT ?s WHERE {
            ?s ?p ?o .
        }
        GROUP BY ?s
        ORDER BY ?s
        LIMIT 1000
        OFFSET 0
    }
    GRAPH %DELETIONS_GRAPH% { ?s ?pred ?obj }
}