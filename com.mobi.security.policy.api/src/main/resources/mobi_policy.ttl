@base <http://mobi.com/ontologies/policy> .

<http://mobi.com/ontologies/policy> a owl:Ontology ;
    rdfs:label "The Mobi Policy Ontology"@en ;
    rdfs:comment "The Mobi Policy ontology describes security access control policies."@en ;
    dct:creator [foaf:name "Stephen Kahmann"], [foaf:name "Megan Mercer"] ;
    foaf:maker [foaf:name "Mobi"; foaf:homepage <https://mobi.inovexcorp.com/>] ;
    dct:modified "2018-01-29"^^xsd:date .

# AllOf

:AllOf a owl:Class ;
    dct:title "All Of"@en ;
    dct:description "A conjunctive collection (logical and) of Match elements."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :match
    ] .

:match a owl:ObjectProperty ;
    dct:title "match"@en ;
    dct:description "A Match to be combined."@en ;
    rdfs:domain :AllOf ;
    rdfs:range :Match .

# AnyOf

:AnyOf a owl:Class ;
    dct:title "Any Of"@en ;
    dct:description "A disjunction collection (logical or) of AllOf elements."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :allOf
    ] .

:allOf a owl:ObjectProperty ;
    dct:title "all of"@en ;
    dct:description "An AllOf to be combined."@en ;
    rdfs:domain :AnyOf ;
    rdfs:range :AllOf .

# Apply

:Apply a owl:Class ;
    dct:title "Apply"@en ;
    dct:description "Describes a function applied to a collection of Expressions."@en ;
    rdfs:subClassOf :Expression, [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :function
    ], [
        a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :expression
    ] .

:function a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "function"@en ;
    dct:description "The Function being applied."@en ;
    rdfs:domain :Apply ;
    rdfs:range :Function .

# AttributeDesignator

:AttributeDesignator a owl:Class ;
    dct:title "Attribute Designator"@en ;
    dct:description "Specifies a specific Attribute from a certain Category and returns the collection of values."@en ;
    rdfs:subClassOf :Expression, [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :attributeId
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :mustBePresent
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :category
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :dataType
    ] .

:attributeId a owl:DatatypeProperty, owl:FunctionalProperty ;
    dct:title "attribute ID"@en ;
    dct:description "The ID of the designated Attribute to retrieve."@en ;
    rdfs:domain :AttributeDesignator ;
    rdfs:range xsd:anyURI .

# AttributeSelector

:AttributeSelector a owl:Class ;
    dct:title "Attribute Selector"@en ;
    dct:description "Selects a specific Attribute based on the path to it from a certain Category and returns the collection of values."@en ;
    rdfs:subClassOf :Expression, [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :category
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :mustBePresent
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :dataType
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :requestContextPath
    ] .

:requestContextPath a owl:DatatypeProperty, owl:FunctionalProperty ;
    dct:title "request context path"@en ;
    dct:description "The path to the selected Attribute."@en ;
    rdfs:domain :AttributeSelector ;
    rdfs:range xsd:string .

# AttributeValue

:AttributeValue a owl:Class ;
    dct:title "Attribute Value"@en ;
    dct:description "A representation of an Attribute value with a datatype."@en ;
    rdfs:subClassOf :Expression, [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :textContent
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :dataType
    ] .

:textContent a owl:DatatypeProperty, owl:FunctionalProperty ;
    dct:title "text content"@en ;
    dct:description "The text for the described AttributeValue."@en ;
    rdfs:domain :AttributeValue ;
    rdfs:range xsd:string .

# CombiningAlgorithm

:CombiningAlgorithm a owl:Class ;
    dct:title "Combining Algorithm"@en ;
    dct:description "A specification of how Rules or Policies should be evaluated and their values combined and returned."@en .

:RuleCombiningAlgorithm a owl:Class ;
    dct:title "Rule Combining Algorithm"@en ;
    dct:description "A specification of how Rules should be evaluated and their values combined and returned."@en ;
    rdfs:subClassOf :CombiningAlgorithm .

:PolicyCombiningAlgorithm a owl:Class ;
    dct:title "Policy Combining Algorithm"@en ;
    dct:description "A specification of how Policies should be evaluated and their values combined and returned."@en ;
    rdfs:subClassOf :CombiningAlgorithm .

xacml3RuleComAlg:deny-overrides a :RuleCombiningAlgorithm, owl:NamedIndividual .
xacml3PolComAlg:deny-overrides a :PolicyCombiningAlgorithm, owl:NamedIndividual .

xacml3RuleComAlg:ordered-deny-overrides a :RuleCombiningAlgorithm, owl:NamedIndividual .

xacml3RuleComAlg:permit-overrides a :RuleCombiningAlgorithm, owl:NamedIndividual .
xacml3PolComAlg:permit-overrides a :PolicyCombiningAlgorithm, owl:NamedIndividual .

xacml3RuleComAlg:ordered-permit-overrides a :RuleCombiningAlgorithm, owl:NamedIndividual .

xacml3RuleComAlg:deny-unless-permit a :RuleCombiningAlgorithm, owl:NamedIndividual .
xacml3PolComAlg:deny-unless-permit a :PolicyCombiningAlgorithm, owl:NamedIndividual .

xacml3RuleComAlg:permit-unless-deny a :RuleCombiningAlgorithm, owl:NamedIndividual .
xacml3PolComAlg:permit-unless-deny a :PolicyCombiningAlgorithm, owl:NamedIndividual .

xacml1RuleComAlg:first-applicable a :RuleCombiningAlgorithm, owl:NamedIndividual .
xacml1PolComAlg:first-applicable a :PolicyCombiningAlgorithm, owl:NamedIndividual .

xacml1PolComAlg:only-one-applicable a :PolicyCombiningAlgorithm, owl:NamedIndividual .

# Condition

:Condition a owl:Class ;
    dct:title "Condition"@en ;
    dct:description "A representation of a function that returns a boolean from its Expression."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :expression
    ] .

# Effect

:Effect a owl:Class ;
    dct:title "Effect"@en ;
    dct:description "An Effect of a Rule, either Permit or Deny."@en ;
    owl:equivalentClass [
        a owl:Class ;
        owl:oneOf (
           :Effect_Permit
           :Effect_Deny
         )
    ] .

:Effect_Permit a :Effect, owl:NamedIndividual .
:Effect_Deny a :Effect, owl:NamedIndividual .

# Expression

:Expression a owl:Class ;
    dct:title "Expression"@en .

# Function

:Function a owl:Class ;
    dct:title "Function"@en ;
    dct:description "A function to apply to a combination of arguments."@en ;
    rdfs:subClassOf :Expression .

