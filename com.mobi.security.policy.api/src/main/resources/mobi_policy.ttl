# Match

:Match a owl:Class ;
    dct:title "Match"@en ;
    dct:description "A specification of an Attribute and its Value to be matched against a request."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :attributeValue
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :matchFunction
    ], [
        a owl:Class ;
        owl:disjointUnionOf (
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :attributeDesignator
            ]
            [
                a owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :attributeSelector
            ]
        )
    ] .

:matchFunction a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "match function"@en ;
    dct:description "The Function to use when matching."@en ;
    rdfs:domain :Match ;
    rdfs:range :Function .

:attributeValue a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "attribute value"@en ;
    dct:description "The AttributeValue to match."@en ;
    rdfs:domain :Match ;
    rdfs:range :AttributeValue .

:attributeSelector a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "attribute selector"@en ;
    dct:description "An AttributeSelector to specify the Attribute with an AttributeValue."@en ;
    rdfs:domain :Match ;
    rdfs:range :AttributeSelector .

:attributeDesignator a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "attribute designator"@en ;
    dct:description "An AttributeDesignator to specify the Attribute with an AttributeValue."@en ;
    rdfs:domain :Match ;
    rdfs:range :AttributeDesignator .

# Policy

:Policy a owl:Class ;
    dct:title "Policy"@en ;
    dct:description "A Collection of Rules to be applied against a request."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :target
    ], [
        a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :rule
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :ruleCombiningAlg
    ] .

:rule a owl:ObjectProperty ;
    dct:title "rule"@en ;
    dct:description "A Rule within the described Policy"@en ;
    rdfs:domain :Policy ;
    rdfs:range :Rule .

:ruleCombiningAlg a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "rule combining algorithm"@en ;
    dct:description "The algorithm to use when combining all the Rules in the described Policy."@en ;
    rdfs:domain :Policy ;
    rdfs:range :RuleCombiningAlgorithm .

# PolicySet

:PolicySet a owl:Class ;
    dct:title "Policy Set"@en ;
    dct:description "A collection of other PolicySets and Policies grouped together to be applied against a request."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :target
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :policyCombiningAlg
    ] .

:policyCombiningAlg a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "policy combining algorithm"@en ;
    dct:description "The algorithm to use when combining all the Policies in the described PolicySet."@en ;
    rdfs:domain :PolicySet ;
    rdfs:range :PolicyCombiningAlgorithm .

:policySet a owl:ObjectProperty ;
    dct:title "policy set"@en ;
    dct:description "Another PolicySet to be combined with the described PolicySet."@en ;
    rdfs:domain :PolicySet ;
    rdfs:range :PolicySet .

:policy a owl:ObjectProperty ;
    dct:title "policy"@en ;
    dct:description "A Policy inside the described PolicySet."@en ;
    rdfs:domain :PolicySet ;
    rdfs:range :Policy .

# Rule

:Rule a owl:Class ;
    dct:title "Rule"@en ;
    dct:description "A collection of assertions to apply against a request and return a verdict."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :effect
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :target
    ], [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty :condition
    ] .

:condition a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "condition"@en ;
    dct:description "A Condition to apply to a request that must be satisfied in order for the described Rule to have its assigned Effect."@en ;
    rdfs:domain :Rule ;
    rdfs:range :Condition .

:effect a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "effect"@en ;
    dct:description "The Effect of the described Rule."@en ;
    rdfs:domain :Rule ;
    rdfs:range :Effect .

# Target

:Target a owl:Class ;
    dct:title "Target"@en ;
    dct:description "A set of Matches within a conjunctive collection (logical and) of AnyOf elements to apply to the request to determine whether the parent element should be applied."@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :anyOf
    ] .

:anyOf a owl:ObjectProperty ;
    dct:title "any of"@en ;
    dct:description "An AnyOf within the described Target."@en ;
    rdfs:domain :Target ;
    rdfs:range :AnyOf .

# Misc.

:dataType a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "data type"@en ;
    dct:description "The Datatype of an AttributeValue."@en ;
    rdfs:domain :AttributeDesignator, :AttributeSelector, :AttributeValue ;
    rdfs:range rdfs:Datatype .

:target a owl:ObjectProperty, owl:FunctionalProperty ;
    dct:title "target"@en ;
    dct:description "The Target of a PolicySet, Policy or Rule."@en ;
    rdfs:domain :Policy, :PolicySet, :Rule ;
    rdfs:range :Target .

:expression a owl:ObjectProperty ;
    dct:title "expression"@en ;
    dct:description "The Expression to Apply or evaluate as a Condition."@en ;
    rdfs:domain :Apply, :Condition ;
    rdfs:range :Expression .

:category a owl:DatatypeProperty, owl:FunctionalProperty ;
    dct:title "category"@en ;
    dct:description "The Category of an Attribute."@en ;
    rdfs:domain :AttributeDesignator, :AttributeSelector ;
    rdfs:range xsd:anyURI .

:mustBePresent a owl:DatatypeProperty, owl:FunctionalProperty ;
    dct:title "must be present"@en ;
    dct:description "Whether or not the designated or selected Attribute must be present. If true, returns \"Indeterminate\", If false, returns an empty collection of values."@en ;
    rdfs:domain :AttributeDesignator, :AttributeSelector ;
    rdfs:range xsd:boolean .

# Datatypes

:rfc822Name a rdfs:Datatype ;
    rdfs:subClassOf xsd:string .

:x500Name a rdfs:Datatype ;
    rdfs:subClassOf xsd:string .

:ipAddress a rdfs:Datatype ;
    rdfs:subClassOf xsd:string .

:dnsName a rdfs:Datatype ;
    rdfs:subClassOf xsd:string .