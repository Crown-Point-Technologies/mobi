<http://mobi.com/ontologies/policy> a owl:Ontology ;
    rdfs:label "The Mobi Policy Ontology"@en ;
    rdfs:comment "The Mobi Policy ontology describes security access control policies."@en;
    dct:creator [foaf:name "Stephen Kahmann"] ;
    foaf:maker [foaf:name "Mobi"; foaf:homepage <https://mobi.inovexcorp.com/>] ;
    dct:modified "2018-01-01"^^xsd:date .

# PolicySet

:PolicySet a owl:Class ;
    dct:title "Policy Set"@en ;
    dct:description "A collection of Policies."@en .

# Policy

:Policy a owl:Class ;
    dct:title "Policy"@en ;
    dct:description "A collection of Rules and Targets."@en .

# Rule

:Rule a owl:Class ;
    dct:title "Rule"@en ;
    dct:description "A collection of Conditions for making policy decisions."@en .

