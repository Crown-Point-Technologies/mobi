@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix mcat: <http://matonto.org/ontologies/catalog#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <http://matonto.org/ontologies/ontology-editor#>.
@base <http://matonto.org/ontologies/ontology-editor>.

<http://matonto.org/ontologies/ontology-editor> a owl:Ontology;
    owl:versionIRI <http://matonto.org/ontologies/ontology-editor#1.0>;
    owl:imports <http://matonto.org/ontologies/catalog>;
    rdfs:label "The MatOnto Ontology Editor Ontology"@en;
    rdfs:comment "The MatOnto Ontology Editor ontology describes the structure of OntologyRecords and how they relate to the MatOnto Catalog."@en;
    dct:creator [foaf:name "Megan Mercer"];
    foaf:maker [foaf:name "MatOnto"; foaf:homepage <http://matonto.org>];
    dct:modified "2017-06-13"^^xsd:date.

# OntologyRecord

:OntologyRecord a owl:Class;
    rdfs:subClassOf mcat:VersionedRDFRecord;
    rdfs:label "Ontology"@en;
    rdfs:comment "A collection of RDF data that defines an ontology resource."@en.

:ontologyIRI a owl:DatatypeProperty, owl:FunctionalProperty;
    rdfs:label "ontology IRI"@en;
    rdfs:comment "The latest ontology IRI on the master branch."@en;
    rdfs:domain :OntologyRecord;
    rdfs:range xsd:anyURI.